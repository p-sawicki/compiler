fun printi : int (i : int);

fun main : int () {
    int i = 0;
    int res = 0;
    while (i < 5) {
        if (not i == 0) {
            if (i > 2) 
                res = printi(5);
            else if (i < 2)
                res = printi(1);
            else
                res = printi(2);
        } else
            res = printi(0);
        i = i + 1;
    }
    return 0;
}